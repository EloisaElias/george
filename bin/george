#!/usr/bin/env python
import sys

def usage():
    print "%s [init-sprint|check-sprint|check-project]" % sys.argv[0]
    exit(1)

if len(sys.argv) != 2:
    usage()
elif sys.argv[1] == 'init-sprint':
    import george.init
    george.init.main()
elif sys.argv[1] == 'check-sprint':
    import george.check_sprint
    george.check_sprint.main()
#elif [ "$1" = 'check-project' ]; then
#    nosetests ~/Zipfian/george/george/check_project.py
else:
    usage()
